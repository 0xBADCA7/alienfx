# UDEV rules for AlienFX controllers found in Alienware computers. These rules
# allow user access of the AlienFX controllers for use by e.g. the alienfx 
# program.

# USB device 0x187c:0x0514 (AlienFX controller for M11xR1 laptop)
SUBSYSTEM=="usb", ATTR{idVendor}=="187c", ATTR{idProduct}=="0514", MODE:="666", GROUP="users"

# USB device 0x187c:0x0515 (AlienFX controller for M11xR2 laptop)
SUBSYSTEM=="usb", ATTR{idVendor}=="187c", ATTR{idProduct}=="0515", MODE:="666", GROUP="users"

# USB device 0x187c:0x0522 (AlienFX controller for M11xR3 laptop)
SUBSYSTEM=="usb", ATTR{idVendor}=="187c", ATTR{idProduct}=="0522", MODE:="666", GROUP="users"

